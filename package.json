{
  "name": "vue-kuanke",
  "version": "1.1.13",
  "description": "Lily管理系统",
  "author": "lihao",
  "private": true,
  "main": "background.js",
  "scripts": {
    "sim": "cross-env NODE_ENV=simulation PROJECT_NAME=all webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev": "cross-env NODE_ENV=development PROJECT_NAME=all webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "ele": "wait-on tcp:8080 && cross-env NODE_ENV=development PROJECT_NAME=all electron . --platform=win32",
    "ele:cli": "wait-on tcp:8080 && cross-env NODE_ENV=development PROJECT_NAME=client electron . --platform=win32",
    "build:wsc": "cross-env NODE_ENV=simulation PROJECT_NAME=client node build/build.js && electron-builder --config build/electron-builder.wsc.json --win",
    "build:wss": "cross-env NODE_ENV=simulation PROJECT_NAME=server node build/build.js && electron-builder --config build/electron-builder.wss.json --win",
    "build:wds": "cross-env NODE_ENV=development PROJECT_NAME=server node build/build.js && electron-builder --config build/electron-builder.wds.json --win",
    "build": "cross-env NODE_ENV=production PROJECT_NAME=all node build/build.js",
    "build:dev": "cross-env NODE_ENV=development PROJECT_NAME=all node build/build.js",
    "build:sim": "cross-env NODE_ENV=simulation PROJECT_NAME=all node build/build.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs"
  },
